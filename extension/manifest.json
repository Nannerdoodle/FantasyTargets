{
  "name": "Fantasy Targets",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "This extension was created with the awesome extensionizr.io",
  "homepage_url": "https://github.com/tcbeutler/FantasyTargets",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "background/js/background.js"
    ],
    "persistent": true
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.google.com/*"
      ],
      "js": [
        "src/inject/inject.js"
      ]
    }
  ],
  "permissions": ["*://*/*", "http://espn.go.com/*", "webRequest", "webRequestBlocking"],
  "web_accessible_resources": ["app/**/*.js", "http://espn.go.com/*"]
}

